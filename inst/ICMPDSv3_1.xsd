	<!-- Text - the only stipulation here is that it does not exceed 250 chars -->
	<xs:simpleType name="text">
		<xs:restriction base="xs:string">
			<xs:maxLength value="250"></xs:maxLength>
			<xs:whiteSpace value="collapse"></xs:whiteSpace>
		</xs:restriction>
	</xs:simpleType>
	<!-- Booleans - either a 1 (meaning 'true') or 0 (meaning 'false') is allowed -->
	<xs:simpleType name="bool">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="1"/>
			<xs:enumeration value="0"/>
		</xs:restriction>
	</xs:simpleType>
  <!-- ICM fields - only A.B.C, A.B.C.D or A.B.C.D.E is a valid format; A in {1, 2}; B = {1-13}; C = {1-16}; D(opt.) = {1-57}; E(opt.) = {1-22} -->
  <xs:simpleType name="icm">
    <xs:restriction base="xs:string">
      <xs:pattern value="[1-2]\.([1-9]|[1][0-3])\.([1-9]|[1][0-6])([\.]([1-9]|[1-5][0-9])([\.]([1-9]|[1-2][0-9])){0,1}){0,1}"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- Postcode (country in some cases) - only alphanumeric characters are allowed, as well as embedded spaces -->
	<xs:simpleType name="postcode"> 
		<xs:restriction base="xs:string">
			<xs:pattern value="[ a-zA-Z0-9]{1,8}"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ADNO types - all values from 20090001 - 20999999 are allowed -->
	<xs:simpleType name="adno">
		<xs:restriction base="xs:unsignedInt">
			<xs:minInclusive value="20090001"/>
			<xs:maxInclusive value="20999999"/>
		</xs:restriction>
	</xs:simpleType>
	<!--START of: Acceptable ranges for all DATE fields follow.....-->
	<!-- DOB: Minimum of 1 January 1900 -->
	<xs:simpleType name="DOBvalue">
		<xs:restriction base="xs:date">
			<xs:minInclusive value="1900-01-01"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- DADEL: Minimum of 20 November 2008 -->
	<xs:simpleType name="DADELvalue">
		<xs:restriction base="xs:date">
			<xs:minInclusive value="2011-11-20"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- DOAH, DAH, DLCCA, DOAICU: Minimum of 1 January 2003 -->
	<xs:simpleType name="PreICUDATEvalue">
		<xs:restriction base="xs:date">
			<xs:minInclusive value="2006-01-01"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- All *OTHER* dates: Minimum of 1 January 2009 -->
	<xs:simpleType name="DATEvalue">
		<xs:restriction base="xs:date">
			<xs:minInclusive value="2012-01-01"/>
		</xs:restriction>
	</xs:simpleType>
	<!--START of: Acceptable ranges for all NUMERIC fields follow.....-->
	  <!-- ANCX: 0.0 - 999.9 -->
	<xs:simpleType name="ANCXvalue">
	    <xs:restriction base="xs:decimal">
	      <xs:minInclusive value="0"/>
	      <xs:maxInclusive value="999.9"/>
	    </xs:restriction>
	</xs:simpleType>
  	<!-- ASSOCIATEDPACO2: 0 - 375.7 -->
	<xs:simpleType name="ASSOCIATEDPACO2value">
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="375.7"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- BLOODGAS: 1 - 794.3 -->
	<xs:simpleType name="BLOODGASvalue">
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="794.3"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- BSA: 0 - 100 -->
	<xs:simpleType name="BSAvalue">
	    <xs:restriction base="xs:unsignedInt">
	      <xs:minInclusive value="0"/>
